<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <RootNamespace>XmfExtractor</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <PackageId>XmfExtractor</PackageId>
    <Authors>Ben Ryves</Authors>
    <Company>Bee Development</Company>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/benryves/XmfExtractor</PackageProjectUrl>
    <RepositoryUrl>https://github.com/benryves/XmfExtractor</RepositoryUrl>
    <Description>Very crude tool to extract data (e.g. MIDI files as SMF) from XMF files</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DotNetZip" Version="1.16.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>